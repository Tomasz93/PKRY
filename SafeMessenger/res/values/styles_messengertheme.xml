<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="AutoCompleteTextViewMessengerTheme" parent="android:Widget.Holo.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/messengertheme_list_selector_holo_dark</item>
	    <item name="android:background">@drawable/messengertheme_edit_text_holo_dark</item>
  </style>

  <style name="ButtonMessengerTheme" parent="android:Widget.Holo.Button">
	  <item name="android:background">@drawable/messengertheme_btn_default_holo_dark</item>
  </style>

  <style name="ImageButtonMessengerTheme" parent="android:Widget.Holo.ImageButton">
	  <item name="android:background">@drawable/messengertheme_btn_default_holo_dark</item>
  </style>

  <style name="SpinnerMessengerTheme" parent="android:Widget.Holo.Spinner">
      <item name="android:background">@drawable/messengertheme_spinner_background_holo_dark</item>
      <item name="android:dropDownSelector">@drawable/messengertheme_list_selector_holo_dark</item>
  </style>

  <style name="TabMessengerTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/messengertheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextMessengerTheme">
	  <item name="android:textColor">#ffffff</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarMessengerTheme" parent="android:Widget.Holo.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/messengertheme_progress_horizontal_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/messengertheme_progress_indeterminate_horizontal_holo_dark</item>
  </style>

  <style name="SeekBarMessengerTheme" parent="android:Widget.Holo.SeekBar">
      <item name="android:progressDrawable">@drawable/messengertheme_scrubber_progress_horizontal_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/messengertheme_scrubber_progress_horizontal_holo_dark</item>
      <item name="android:thumb">@drawable/messengertheme_scrubber_control_selector_holo_dark</item>
  </style>

  <style name="RatingBarMessengerTheme" parent="android:Widget.Holo.RatingBar">
      <item name="android:progressDrawable">@drawable/messengertheme_ratingbar_full_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/messengertheme_ratingbar_full_holo_dark</item>
  </style>

  <style name="RatingBarBigMessengerTheme" parent="android:Widget.Holo.RatingBar.Indicator">
      <item name="android:progressDrawable">@drawable/messengertheme_ratingbar_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/messengertheme_ratingbar_holo_dark</item>
  </style>

  <style name="RatingBarSmallMessengerTheme" parent="android:Widget.Holo.RatingBar.Small">
      <item name="android:progressDrawable">@drawable/messengertheme_ratingbar_small_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/messengertheme_ratingbar_small_holo_dark</item>
  </style>

  <style name="ToggleMessengerTheme" parent="android:Widget.Holo.Button.Toggle">
      <item name="android:background">@drawable/messengertheme_btn_toggle_holo_dark</item>
  </style>

</resources>